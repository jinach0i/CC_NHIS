}if(ttsenv_firstpage==true){webgen_onoutinfo(ttsenv_information,false,false)}}function webgen_init_ie(){webgen_checkmaster();
f_setVoice();if(ttsenv_opt_usescalescreen==true){f_setInit()}if(ttsenv_firstpage==true){webgen_onoutinfo(ttsenv_information,false,false)
}}if(ttsenv_auto_onload==true){var mode=false;if(document!=null){var state=document.readyState;if(state!=null){state=state.toLowerCase(state);
if(state=="loaded"){mode=true}else{if(state=="complete"){mode=true}}}}if(mode==true){if(webgen_browser.msie==true){webgen_init_ie()
}else{webgen_init_ff()}}else{if(this.addEventListener){this.addEventListener("load",webgen_init_ff,true)
}else{this.attachEvent("onload",webgen_init_ie)}}};
